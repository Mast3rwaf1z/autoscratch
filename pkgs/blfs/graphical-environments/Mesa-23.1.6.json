{
	"name":"mesa-23.1.6",
	"src":"https://mesa.freedesktop.org/archive/mesa-23.1.6.tar.xz",
	"config":[
		"mkdir build",
		"cd build",
		"meson setup --prefix=$XORG_PREFIX --buildtype=release -Dplatforms=x11 -Dgallium-drivers=auto -Dvulkan-drivers=\"\" -Dvalgrind=disabled -Dlibunwind=disabled .."
	],
	"build":[
		"cd build",
		"ninja"
	],
	"install":[
		"cd build",
		"ninja install"
	],
	"uninstall":[
		"cd build",
		"ninja uninstall"
	]
}
