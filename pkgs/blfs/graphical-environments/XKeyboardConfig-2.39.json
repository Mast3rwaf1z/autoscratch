{
  "name":"xkeyboard-config-2.39",
  "src":"https://www.x.org/pub/individual/data/xkeyboard-config/xkeyboard-config-2.39.tar.xz",
  "config":[
    "mkdir build",
    "cd    build",
    "meson setup --prefix=$XORG_PREFIX --buildtype=release .."
  ],
  "build":[
    "cd build",
    "ninja"
  ],
  "install":[
    "cd build",
    "ninja install"
  ],
  "uninstall":[
    "ninja uninstall"
  ]
}
