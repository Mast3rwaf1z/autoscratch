{
	"name":"gtk+-3.24.38",
	"src":"https://download.gnome.org/sources/gtk+/3.24/gtk+-3.24.38.tar.xz",
	"config":[
		"mkdir build",
		"cd build",
		"meson setup --prefix=/usr --buildtype=release -Dman=true -Dbroadway_backend=true -Dwayland_backend=false -Dx11_backend=true .."
	],
	"build":[
		"cd build",
		"ninja"
	],
	"install":[
		"cd build",
		"ninja install"
	],
	"uninstall":[
		"cd build",
		"ninja uninstall"
	]
}
