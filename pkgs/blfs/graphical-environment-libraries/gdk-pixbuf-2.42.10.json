{
	"name":"gdk-pixbuf-2.42.10",
	"src":"https://download.gnome.org/sources/gdk-pixbuf/2.42/gdk-pixbuf-2.42.10.tar.xz",
	"config":[
		"mkdir build",
		"cd build",
		"meson setup .. --prefix=/usr --buildtype=release --wrap-mode=nofallback"
	],
	"build":[
		"cd build",
		"ninja"
	],
	"install":[
		"cd build",
		"ninja install"
	],
	"uninstall":[
		"cd build",
		"ninja uninstall"
	]
}
