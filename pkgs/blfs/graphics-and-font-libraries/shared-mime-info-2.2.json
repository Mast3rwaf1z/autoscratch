{
	"name":"shared-mime-info-2.2",
	"src":"https://gitlab.freedesktop.org/xdg/shared-mime-info/-/archive/2.2/shared-mime-info-2.2.tar.gz",
	"config":[
		"mkdir build",
		"cd build",
		"meson setup --prefix=/usr --buildtype=release -Dupdate-mimedb=true .."
	],
	"build":[
    "cd build",
		"ninja"
	],
	"install":[
    "cd build",
		"ninja install"
	],
	"uninstall":[
    "cd build",
		"ninja uninstall"
	]
}
