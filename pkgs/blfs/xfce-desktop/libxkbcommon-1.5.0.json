{
  "name":"libxkbcommon-1.5.0",
  "src":"https://xkbcommon.org/download/libxkbcommon-1.5.0.tar.xz",
  "config":[
    "mkdir build",
    "cd    build",
    "meson setup .. --prefix=/usr --buildtype=release -Denable-docs=false -Denable-wayland=false"
  ],
  "build":[
    "ninja"
  ],
  "install":[
    "ninja install"
  ],
  "uninstall":[
    "ninja uninstall"
  ]
}
