{
  "name":"yajl-2.1.0",
  "src":["https://github.com/lloyd/yajl/archive/refs/tags/2.1.0.tar.gz"],
  "config":[
    "mkdir build",
    "cd build",
    "cmake .. --install-prefix=/usr"
  ],
  "build":[
    "cd build",
    "make"
  ],
  "install":[
    "cd build",
    "make install" 
  ],
  "uninstall":[
    "cd build",
    "make uninstall"
  ]
}
