{
  "name":"pango-1.50.14",
  "src":"https://download.gnome.org/sources/pango/1.50/pango-1.50.14.tar.xz",
  "cmds":[
    "sed -i '/array-bounds/d' meson.build",
    "mkdir build",
    "cd build",
    "meson setup --prefix=/usr --buildtype=release --wrap-mode=nofallback ..",
    "ninja",
    "ninja install"
  ]
}
